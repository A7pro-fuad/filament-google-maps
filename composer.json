{
    "name": "cheesegrits/filament-google-maps",
    "description": "A Google Maps package for Filament PHP with field, column and widget",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/cheesegrits/filament-google-maps",
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9",
        "filament/filament": "^2.0",
        "guzzlehttp/guzzle": "^7.5",
        "geocoder-php/google-maps-provider": "^4.7",
        "php-http/guzzle7-adapter": "^1.0",
        "spatie/guzzle-rate-limiter-middleware": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Cheesegrits\\FilamentGoogleMaps\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cheesegrits\\FilamentGoogleMaps\\FilamentGoogleMapsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Hugh Messenger",
            "email": "hugh.messenger@gmail.com"
        }
    ]
}
